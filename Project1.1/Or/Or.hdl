 /**
 * Or gate:
 * out = 1 if (a == 1 or b == 1)
 *       0 otherwise
 */

CHIP Or {
    IN a, b;
    OUT out;

    PARTS:
    Nand(a = a, b = a, out = Nanda);
    Nand(a = b, b = b, out = Nandb);
    Nand(a = Nanda, b = Nandb, out = out);
}
